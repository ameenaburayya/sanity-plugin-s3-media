{
  "name": "sanity-plugin-s3-media",
  "version": "1.2.0",
  "description": "Adding the ability to have s3 bucket stored media",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "license": "MIT",
  "author": "Ameen Aburayya <a.aburayya95@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ameenaburayya/sanity-plugin-s3-media.git"
  },
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./client": {
      "source": "./src/client.ts",
      "import": "./dist/client.mjs",
      "default": "./dist/client.js"
    },
    "./asset-utils": {
      "source": "./src/asset-utils.ts",
      "import": "./dist/asset-utils.mjs",
      "default": "./dist/asset-utils.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "test": "vitest run",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.0",
    "@sanity/client": "^7.13.2",
    "@sanity/color": "^3.0.6",
    "@sanity/icons": "^3.7.0",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/studio-secrets": "^3.0.3",
    "@sanity/ui": "^3.0.5",
    "@sanity/uuid": "^3.0.2",
    "@tanem/react-nprogress": "^5.0.55",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^4.0.0",
    "exif-component": "^1.0.1",
    "filesize": "^11.0.13",
    "groq": "^3.0.0",
    "is-hotkey": "^0.2.0",
    "lodash": "^4.17.23",
    "pluralize": "^8.0.0",
    "react-dropzone": "^11.3.1",
    "react-file-icon": "^1.6.0",
    "react-redux": "^9.2.0",
    "react-rx": "^4.2.2",
    "react-virtuoso": "^4.12.5",
    "redux": "^5.0.1",
    "redux-observable": "3.0.0-rc.2",
    "rxjs": "^7.8.1",
    "sanity": "^5.8.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.29.0",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^9.39.2",
    "@sanity/pkg-utils": "^10.4.4",
    "@sanity/plugin-kit": "^4.0.20",
    "@sanity/semantic-release-preset": "^6.0.0",
    "@semantic-release/github": "^12.0.3",
    "@types/is-hotkey": "^0.1.10",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.2.0",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/react-file-icon": "^1.0.4",
    "@types/react-is": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-is": "^19.2.4",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "styled-components": "^6.3.8",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19",
    "react-is": "^19",
    "sanity": "^5",
    "styled-components": "^6.1"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=18"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false
    }
  }
}
